{
    "version": 2,
    "builds": [
      {
          "src": "server.js",
          "use": "@vercel/node"
        },
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
          "src": "/(.*)",
          "dest": "server.js"
        },
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.html"
      }
    ]
  }